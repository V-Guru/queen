<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="@dimen/uniform_spacing">
    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/connect_button_width"
        android:layout_height="@dimen/connect_button_width"
        android:scaleType="fitXY"
        android:id="@+id/channelImage"
        android:background="@color/white"
        android:padding="@dimen/elevation"
        app:strokeColor="@color/gray_light1"
        app:strokeWidth="1dp"
        app:shapeAppearance="@style/Circleimage"
        android:src="@drawable/ic_starplus"/>

</RelativeLayout>